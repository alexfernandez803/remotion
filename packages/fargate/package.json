{
	"name": "@remotion/fargate",
	"version": "4.0.27",
	"description": "AWS Fargate alternative to lambda rendering",
	"main": "dist/index.js",
	"scripts": {
		"build": "tsc -d && pnpm run buildContainer && pnpm run tarInstaller",
		"buildContainer": "ts-node src/admin/bundle-renderLogic.ts",
		"tarInstaller": "ts-node src/admin/bundle-installer.ts",
		"watch": "tsc -w",
		"lint": "eslint src --ext ts,tsx",
		"test": "vitest --run",
		"publish": "cd container && ARTIFACT_REGISTRY_ENV=production node submit.mjs",
		"serve": "ts-node src/functions/server.ts"
	},
	"dependencies": {
		"@aws-sdk/client-cloudwatch-logs": "3.382.0",
		"@aws-sdk/client-iam": "3.382.0",
		"@aws-sdk/client-lambda": "3.382.0",
		"@aws-sdk/client-s3": "3.382.0",
		"@aws-sdk/client-service-quotas": "3.382.0",
		"@aws-sdk/client-sts": "3.382.0",
		"@aws-sdk/credential-providers": "3.382.0",
		"@remotion/bundler": "workspace:*",
		"@remotion/cli": "workspace:*",
		"@remotion/renderer": "workspace:*",
		"aws-policies": "^1.0.1",
		"google-auth-library": "^8.7.0",
		"http": "0.0.1-security",
		"mime-types": "2.1.34",
		"remotion": "workspace:*",
		"zod": "^3.21.4"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.266",
		"@remotion/compositor-linux-x64-gnu": "workspace:*",
		"@types/minimist": "1.2.2",
		"@types/node": "^18.11.2",
		"eslint": "8.42.0",
		"prettier": "3.0.2",
		"prettier-plugin-organize-imports": "^3.2.2",
		"ts-node": "^10.8.0",
		"vitest": "0.24.3"
	},
	"exports": {
		"./package.json": "./package.json",
		".": "./dist/index.js",
		"./defaults": "./dist/defaults.js",
		"./regions": "./dist/regions.js"
	},
	"typesVersions": {
		">=1.0": {
			"defaults": [
				"dist/defaults.d.ts"
			],
			"regions": [
				"dist/regions.d.ts"
			]
		}
	}
}
