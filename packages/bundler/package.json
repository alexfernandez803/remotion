		"style-loader": "2.0.0",
		"webpack": "5.83.1"
	},
	"peerDependencies": {
		"react": ">=16.8.0",
		"react-dom": ">=16.8.0"
	},
	"devDependencies": {
		"@jonny/eslint-config": "3.0.266",
		"@types/node": "18.14.6",
		"bun-types": "0.8.0",
		"@types/react": "18.0.26",
		"@types/react-dom": "18.0.10",
		"eslint": "8.42.0",
		"eslint-plugin-10x": "1.5.2",
		"eslint-plugin-react": "7.32.2",
		"eslint-plugin-react-hooks": "4.4.0",
		"prettier": "3.0.2",
		"prettier-plugin-organize-imports": "^3.2.2",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"vitest": "0.31.1"
