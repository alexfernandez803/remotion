{
	"name": "@remotion/bundler",
	"version": "4.0.27",
	"description": "Bundler for Remotion",
	"main": "dist/index.js",
	"sideEffects": false,
	"repository": {
		"url": "https://github.com/remotion-dev/remotion"
	},
	"bugs": {
		"url": "https://github.com/remotion-dev/remotion/issues"
	},
	"scripts": {
